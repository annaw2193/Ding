     {this.createCheckboxes()}\n                    </div>\n\n                    <label>\n                        Number of Page Elements\n                    </label>\n                    <input type='number' max='50' step='1' value={this.state.num} onChange={this.handleNumChange}></input>\n        \n                    <div className=\"form-group mt-2\">\n                    <Button type=\"submit\" className=\"btn generate\" size='lg' block>\n                        DING\n                    </Button>\n                    </div>\n                </form>\n                </div>\n            </div>\n            </div>\n        );\n    }\n};\n\nexport default Input;\n","/Users/RyanChan/Desktop/projects/Ding/frontend/src/Checkbox.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":4,"column":8,"nodeType":"37","messageId":"38","endLine":4,"endColumn":17},"no-native-reassign",["39"],"no-negated-in-lhs",["40"],"no-unused-vars","'Container' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]